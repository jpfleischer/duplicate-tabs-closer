{
	"manifest_version": 2,
	"name": "DupTabX",
	"version": "1.0.0",
	"author": "jpfleischer",
	"homepage_url": "https://github.com/jpfleischer/duplicate-tabs-closer/",
	"default_locale": "en",
	"description": "The best way to manage duplicate tabs.",
	"icons": {
		"48": "images/auto_close_32.png"
	},
	"applications": {
		"gecko": {
			"id": "77b3f8b0-10ef-47c6-b5f6-b6e43f036d1d@jpfleischer",
			"strict_min_version": "68.0"
		}
	},
	"background": {
		"scripts": [
			"helper.js",
			"tabsInfo.js",
			"options.js",
			"urlUtils.js",
			"badge.js",
			"worker.js",
			"messageListener.js",
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": false,
		"default_icon": {
			"16": "images/auto_close_16.png",
			"32": "images/auto_close_32.png"
		},
		"default_title": "DupTabX",
		"default_popup": "popup/popup.html"
	},
	"permissions": [
		"cookies",
		"tabs",
		"webNavigation",
		"storage",
		"contextualIdentities"
	],
	"commands": {
		"close-duplicate-tabs": {
			"suggested_key": {
				"default": "Alt+Shift+W"
			},
			"description": "Close all duplicate tabs"
		},
		"toggle-close-automatically": {
			"suggested_key": {
				"default": "Alt+Shift+D"
			},
			"description": "Toggle close automatically"
		}
	},
	"options_ui": {
		"page": "optionPage/optionPage.html",
		"browser_style": false,
		"open_in_tab": true
	}
}
